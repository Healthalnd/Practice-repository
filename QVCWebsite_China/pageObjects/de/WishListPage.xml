<?xml version="1.0" encoding="UTF-8"?>
<Page name="WishListPage" include="MastheadMain">

	<TestObject name="pleaseNoteMessage">
		<Properties>
			<css>p.phone-hidden</css>
			<id>divWishList</id>
		</Properties>
	</TestObject>
	<TestObject name="itemNumber">
		<Properties>
			<xpath>//*[@class='divItemNumber'][1]</xpath>
			<xpath>//*[@data-component-type="WISH_LIST_ELEMENTS"]//*[contains(@class,'wishListCardWrapper')][1]//img[contains(@title,'&lt;text&gt;')]</xpath>
		</Properties>
	</TestObject>
	<TestObject name="removeBtn">
		<Properties>
			<xpath>//*[@class="wishListCard"][1]//*[@class="removeItemWL"]</xpath>
			<xpath>//*[@class='divProductButtons']/a[1]</xpath>
		</Properties>
	</TestObject>
	
	<TestObject name="ColorOption">
		<Properties>
			<xpath>//*[@class="selectColor"][contains(@content,"&lt;Color&gt;")]</xpath>
			<xpath>//*[@class="selectColor"]//*[contains(text(),'&lt;Color&gt;')]</xpath>
			<xpath>//*[@class='productOption'][1]</xpath>
		</Properties>
	</TestObject>
	<TestObject name="SizeOption">
		<Properties>
			<xpath>//*[contains(@class, 'wishlSize')]//*[.='&lt;Size&gt;']</xpath>
			<xpath>//*[@class='productOption'][1]</xpath>
		</Properties>
	</TestObject>
</Page>