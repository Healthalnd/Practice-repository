<?xml version="1.0" encoding="UTF-8"?>
<Page name="ForgottenYourPasswordPage" include="MastheadMain">

	<TestObject name="pageTitle">
		<Properties>
			<xpath>//*[@id="divPageTitle"]</xpath>
		</Properties>
	</TestObject>
	<TestObject name="emailAddressText">
		<Properties>			
			<id>lblLogonId</id>
		</Properties>
	</TestObject>
	<TestObject name="emailAddressTextbox">
		<Properties>			
			<id>txtLogonId</id>
